        <!-- Generate reply to synchronous request -->
        <bpel:flow name="ParalellFlow"><bpel:sequence name="CheckAvailability">

                
                <bpel:assign validate="no" name="AssignInputToAvailabilityRequest">
                    <bpel:copy ignoreMissingFromData="no">
                        <bpel:from><bpel:literal><FlowSOG:checkAvailabilityAsync xmlns:FlowSOG="http://iaas.uni-stuttgart.de/labs/FlowSOG" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
