                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    <![CDATA[$OrderLinkCostResponse.calcOrderCostsOutput/ns:orderCosts+$ShipmentLinkResponse.calcShipmentCostsOutput/ns:shipmentCosts+$PaymentLinkResponse.calcTransactionCostsOutput/ns:transactionCosts]]>
                </bpel:from>
                <bpel:to part="conductPaymentInput" variable="PaymentLinkPayRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:amount]]></bpel:query>
