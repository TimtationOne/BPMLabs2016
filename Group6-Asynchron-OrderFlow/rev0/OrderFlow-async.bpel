                    </bpel:scope>
                </bpel:forEach>
                
            </bpel:sequence><bpel:sequence name="ProcessOrderDetails">
                
                
                <bpel:if name="IfOrderIdExists"><bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[boolean($input.payload/tns:input/tns:orderId)]]></bpel:condition>
                    <bpel:assign validate="no" name="AssignOrderId">
