                    </bpel:correlations>
                
                </bpel:invoke>
                
                <bpel:receive name="ReceiveAvailabilityCallback" partnerLink="InventoryCallbackLink" operation="onFinishCheckAvailability" portType="ns:InventoryCallbackPortType" variable="InventoryCallbackLinkResponse">
