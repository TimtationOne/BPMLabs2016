                                                </bpel:query>
                                            </bpel:from>
                                            <bpel:to variable="NotAvailableProduct"></bpel:to>
                                        </bpel:copy>
                                    
                            </bpel:assign>
                            <bpel:if name="IfAProductIsNotAvailable">
