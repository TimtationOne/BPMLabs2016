                        </bpel:to>
                    </bpel:copy>
                    
                </bpel:assign>
                
                
                
                
                
                
                
                
                <bpel:invoke name="InvokeAvailabilityRequest" partnerLink="InventoryCallbackLink" operation="checkAvailabilityAsync" portType="ns:InventoryPortType" inputVariable="InventoryLinkRequest">
